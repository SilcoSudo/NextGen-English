# Production Environment Variables for NextGen English Frontend
REACT_APP_API_URL=https://api.nextgenenglish.id.vn
REACT_APP_DOMAIN=nextgenenglish.id.vn
REACT_APP_ENV=production

# Analytics (if needed)
REACT_APP_GA_TRACKING_ID=your_google_analytics_id_here

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true

# CDN or Static Assets (if using)
REACT_APP_CDN_URL=https://cdn.nextgenenglish.id.vn

# API Endpoints
REACT_APP_VIDEO_API_URL=https://api.nextgenenglish.id.vn/api/videos
REACT_APP_IMAGE_API_URL=https://api.nextgenenglish.id.vn/api/images